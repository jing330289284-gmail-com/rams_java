<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="jp.co.lyc.cms.mapper.BranchInsertMapper">
	<!-- データの存在チェック -->
	<select id="checkHave" parameterType="map"
		resultType="java.lang.String">
		SELECT * FROM M002BankBranch WHERE bankCode = #{bankCode} and bankBranchCode=#{bankBranchCode}
	</select>
	<!-- データのインサート -->
	<insert id="insertMaster" parameterType="map">
		INSERT INTO M002BankBranch (bankBranchCode,bankCode,bankBranchName,updateTime,createTime,updateUser) VALUE(#{bankBranchCode},#{bankCode},#{bankBranchName},NOW(),NOW(),#{updateUser})
	</insert>
</mapper>